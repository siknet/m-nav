{
  "name": "m-nav",
  "version": "0.4.0",
  "private": true,
  "keywords": [
    "m-nav",
    "notion",
    "navigation",
    "nextjs",
    "react",
    "typescript"
  ],
  "scripts": {
    "build": "turbo build",
    "dev": "turbo dev",
    "lint": "turbo lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "prepare": "husky && git config --local core.editor cat",
    "release": "HUSKY=0 bumpp package.json apps/*/package.json --all"
  },
  "devDependencies": {
    "@m-nav/eslint-config": "workspace:*",
    "@m-nav/typescript-config": "workspace:*",
    "bumpp": "^10.2.0",
    "eslint": "^9.20.1",
    "prettier": "^3.5.1",
    "turbo": "^2.4.2",
    "typescript": "5.7.3"
  },
  "packageManager": "pnpm@10.4.1",
  "engines": {
    "node": ">=20"
  },
  "dependencies": {
    "husky": "^9.1.7"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "core-js-pure",
      "sharp"
    ]
  }
}