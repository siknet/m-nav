{
  "name": "m-nav",
  "version": "0.4.0",
  "private": true,
  "keywords": [
    "m-nav",
    "notion",
    "navigation",
    "nextjs",
    "react",
    "typescript"
  ],
  "scripts": {
    "build": "turbo build",
    "dev": "turbo dev",
    "lint": "turbo lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "prepare": "git config --local core.editor cat",
    "release": "LEFTHOOK=0 bumpp package.json apps/*/package.json --all"
  },
  "devDependencies": {
    "@m-nav/eslint-config": "workspace:*",
    "@m-nav/typescript-config": "workspace:*",
    "bumpp": "catalog:",
    "eslint": "catalog:",
    "lefthook": "catalog:",
    "prettier": "catalog:",
    "turbo": "catalog:",
    "typescript": "catalog:"
  },
  "packageManager": "pnpm@10.4.1",
  "engines": {
    "node": ">=20"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "lefthook",
      "core-js-pure",
      "sharp"
    ]
  }
}